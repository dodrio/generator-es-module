{
  "name": "generator-nm",
  "version": "5.0.1",
  "description": "Scaffold out a node module",
  "license": "MIT",
  "repository": "m31271n/generator-nm",
  "author": {
    "name": "m31271n",
    "email": "m31271n@gmail.com",
    "url": "index.m31271n.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
