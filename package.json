    "humanize-url": "^1.0.1",
    "is-scoped": "^1.0.0",
    "normalize-url": "^1.2.0",
    "underscore.string": "^3.0.3",
    "yeoman-generator": "^1.0.1"
  },
  "devDependencies": {
    "ava": "*",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "pify": "^3.0.0",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.0.0"
  },
  "ava": {
    "failWithoutAssertions": false
