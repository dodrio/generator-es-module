{
  "name": "@m31271n/generator-es-module",
  "version": "7.1.0",
  "description": "Scaffold out an ECMAScript module.",
  "license": "MIT",
  "repository": "m31271n/generator-es-module",
  "author": {
    "name": "m31271n",
    "email": "m31271n@gmail.com",
    "url": "stack.m31271n.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "test": "ava",
    "code-quality": "npm run lint && npm run test",
    "release": "npm run code-quality && npm publish"
  },
  "files": [
    "app"
  ],
  "keywords": [
    "yeoman-generator",
    "boilerplate",
    "init",
    "template",
    "scaffold",
    "ECMAScript",
    "ES",
    "javascript",
    "node",
    "module",
    "node_module",
    "node-module"
  ],
  "dependencies": {
    "humanize-url": "^2.0.0",
    "is-scoped": "^1.0.0",
    "normalize-url": "^4.0.0",
    "underscore.string": "^3.3.5",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "ava": "1.0.0-rc.1",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "pify": "^4.0.1",
    "prettier": "^1.15.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "ava": {
    "failWithoutAssertions": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [
      "app/templates/**"
    ],
    "linters": {
      "**/*.{json,md}": [
        "prettier --write",
        "git add"
      ],
      "**/*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  }
}
